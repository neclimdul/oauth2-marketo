{
    "name": "neclimdul/oauth2-marketo",
    "description": "Marketo OAuth 2.0 support for the PHP League's OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "marketo",
        "marketo api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kristen Kehlenbeck",
            "email": "klkehlenbeck@gmail.com"
        },
        {
            "name": "James Gilliland",
            "email": "neclimdul@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "league/oauth2-client": "^1.0 | ^2.0",
        "phpspec/prophecy": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "NecLimDul\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NecLimDul\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
